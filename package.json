    "type": "git",
    "url": "https://github.com/fweiss/etappe"
  },
  "dependencies": {
    "angular": "1.4.4",
    "underscore": "1.8.3"
  },
  "devDependencies": {
    "angular-mocks": "1.4.4",
    "http-server": "^0.8.5",
    "mountebank": "^1.4.2",
    "protractor": "^4.0.14",
    "request": "^2.67.0",

    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
