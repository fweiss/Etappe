{
    "name": "etappe",
    "version": "1.0.0",
    "repository": "https://github.com/angular/angular-seed",
    "devDependencies": {
        "karma": "~0.12.0",
        "karma-jasmine": "~0.2.0",
        "karma-spec-reporter": "~0.0.13",
        "karma-jasmine-html-reporter": "~0.1",
        "http-server": "^0.6.1",
        "protractor": "^1.1.1",
        "mountebank": "~1.2.45",
        "request": "~2.53.0"
    },
    "scripts": {
        "prestart": "npm install http-server",
        "start": "http-server -a localhost -p 8080 -c-1",
